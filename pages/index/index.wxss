swiper,.slide-image{
  width:100%;
  height:351rpx;
}
swiper{
  margin-bottom:6rpx;
}
.search{
  position: relative;
  height:120rpx;
}
.search_input{
  position: absolute;
  display: block;
  top:50%;
  left:50%;
  margin-left:-212rpx;
  width:414rpx;
  margin-top:-45rpx;
  font-size: 36rpx;
  padding: 18rpx;
  text-align: center;
  background-color: rgba(255, 255, 255, 0.8)
}
.header{
  width:100%;
  height:120rpx;
}

.key_h{
  font-size: 36rpx;
  display: inline-block;
  vertical-align: top;
}
.key{
  padding:18rpx;
}
.key_btn{
  margin-right:16rpx;
  border-radius: 50rpx;
  vertical-align: top;
}
button.key_btn::after {
  border: 0;
}
.index_main{
  display: flex;

  flex-wrap:wrap;
  justify-content:space-between;
}
.item_navigator{
  width:33%;
  margin-bottom:16rpx;
}
.item_index{
  width:100%;
}
.item_image{
  width:100%;
  height:300rpx;
  display: block;
  position: relative;
}
.item_image text{
  position: absolute;
  right: 4rpx;
  bottom:2rpx;
  color:#fff;

}
.item_title{
  font-size: 28rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  line-height: 2;
  text-indent: 0.4em;
}
.biaoti{
  font-size: 36rpx;
  display: block;
  padding:18rpx;
}